{
	"name": "TrixieUiKit",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"start": "node node_modules/react-native/local-cli/cli.js start",
		"precommit": "lint-staged",
		"lint": "eslint pages components --quiet --ext .jsx --ext .js",
		"format": "prettier --config=./.prettierrc --write './pages/**/*.js' './components/**/*.js'"
	},
	"lint-staged": {
		"gitDir": "../",
		"linters": {
			"TrixieUiKit/components/**/*.{js,jsx}": "eslint"
		}
	},
	"dependencies": {
		"prop-types": "^15.6.0",
		"react": "16.0.0-beta.5",
		"react-native": "0.49.3",
		"react-native-back-android": "^3.1.0",
		"react-native-linear-gradient": "^2.3.0",
		"react-native-simple-radio-button": "^2.7.0",
		"react-navigation": "^1.0.0-beta.13"
	},
	"devDependencies": {
		"babel-eslint": "^8.0.1",
		"babel-jest": "21.2.0",
		"babel-preset-react-native": "4.0.0",
		"eslint": "^4.9.0",
		"eslint-config-airbnb": "^16.1.0",
		"eslint-config-prettier": "^2.9.0",
		"eslint-plugin-import": "^2.8.0",
		"eslint-plugin-jsx-a11y": "^6.0.2",
		"eslint-plugin-react": "^7.4.0",
		"eslint-plugin-react-native": "^3.1.0",
		"husky": "^0.14.3",
		"jest": "21.2.1",
		"lint-staged": "^4.3.0",
		"prettier": "1.11.1",
		"react-test-renderer": "16.0.0-beta.5"
	},
	"jest": {
		"preset": "react-native"
	}
}
